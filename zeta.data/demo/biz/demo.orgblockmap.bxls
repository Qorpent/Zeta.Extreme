	namespace blocks

		~class eco 'map-own' owner=^demo.org.ub 
			~block fin curator=maincurator
			
		~class proizv 'map-own' owner=^demo.org.proizv 
		
		~class personal 'map-own' owner=^demo.org.personal
		
		
		class themaindex "Индекс тем и автотем" prototype=themas
			export themas
			include all formdef body
				where localname=thema
				groupby block as=block
				select root=true colset=true
	
	